<Window x:Class="Kooste.Window_browser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kooste"
        mc:Ignorable="d"
        Title="Window_browser" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="http://" VerticalAlignment="Top" Width="679"/>
        <Button x:Name="go" Content="Go" HorizontalAlignment="Left" Margin="694,10,0,0" VerticalAlignment="Top" Width="88" Height="23" Click="go_Click"/>
        <WebBrowser x:Name="webBrowser" HorizontalAlignment="Left" Height="371" Margin="10,38,0,0" VerticalAlignment="Top" Width="772"/>

    </Grid>
</Window>
